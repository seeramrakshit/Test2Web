@model List<Category> 

<h1>Categort List</h1>

<table class="table table-bordered table-striped table-dark">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Display Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOder))
        {
            <tr>
                <td>@obj.Name </td>
                <td>@obj.DisplayOder</td>
            </tr>
        }
    </tbody>
</table>